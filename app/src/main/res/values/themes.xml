<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.QuizzlyPlay" parent="android:Theme.Material.Light.DarkActionBar" >


    <item name="colorPrimary" tools:ignore="DuplicateDefinition">@color/purple_500</item>
    <item name="colorPrimaryVariant" tools:ignore="DuplicateDefinition">@color/purple_700</item>
    <item name="colorOnPrimary" tools:ignore="DuplicateDefinition">@color/white</item>

    <item name="colorSecondary" tools:ignore="DuplicateDefinition">@color/teal_200</item>
    <item name="colorSecondaryVariant" tools:ignore="DuplicateDefinition">@color/teal_700</item>
    <item name="colorOnSecondary" tools:ignore="DuplicateDefinition">@color/black</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>


    </style>


    <style name="AppThemeSplash" parent="android:Theme.Material.Light.NoActionBar" >
        <item name="colorPrimary" tools:ignore="DuplicateDefinition">@color/orange</item>
        <item name="colorPrimaryVariant" tools:ignore="DuplicateDefinition">@color/orange</item>
        <item name="colorOnPrimary" tools:ignore="DuplicateDefinition">@color/white</item>

        <item name="colorSecondary" tools:ignore="DuplicateDefinition">@color/teal_200</item>
        <item name="colorSecondaryVariant" tools:ignore="DuplicateDefinition">@color/teal_700</item>
        <item name="colorOnSecondary" tools:ignore="DuplicateDefinition">@color/black</item>

        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>

    </style>
</resources>